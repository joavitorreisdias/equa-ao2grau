#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <math.h>  //  Arquivo de cabe√ßalho (header)
void main()
{
    float B;
    float A;
    float C;
    float x1;
    float x2;


    printf("Digite A: ");
    scanf("%f", &A);
    printf("Digite  B: ");
    scanf("%f", &B);
    printf("Digite  C: ");
    scanf("%f", &C);
    x1 = -(B/2.0)+ sqrt(B*B-4.0*A*C)/2.0*A;
    x2 = -(B/2.0)- sqrt(B*B-4.0*A*C)/2.0*A;
    printf(" x1 vale : %f", x1);
    printf(" x2 vale : %f", x2);
}
